<% if(success && success.length){ %>
    <div class="alert alert-success d-flex justify-content-between align-items-center text-center" 
    role="alert" 
    style="font-size: 1.2rem;">
   <span class="flex-grow-1"><%= success %></span> 
   <button
       type="button"
       class="btn-close"
       data-bs-dismiss="alert"
       aria-label="Close"
   ></button>
</div>